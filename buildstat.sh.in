#!/bin/sh

exec 2>&1

echo "Date is:"
date | awk '{ print $2, $3, $NF }'
echo ""

echo "System is:"
uname -sr
triplet=`./config.guess`
echo "($triplet)"
echo ""

if [ -x ./vmips ]; then
	echo "Congratulations, you have successfully compiled: "
	./vmips --version
else
	echo "Your sources are version: "
	cat VERSION
fi
echo ""

echo "Host tools (g++) version: "
@CXX@ -v
echo ""

echo "MIPS tools (gcc) version and target: "
@mipstoolprefix@gcc --version
@mipstoolprefix@gcc -dumpmachine
echo ""

echo "MIPS tools (binutils) version: "
@mipstoolprefix@ld --version | head -1
echo ""
 
echo "MIPS BFD target: "
echo "@mipsbfdtarget@"
echo ""

exit 0
