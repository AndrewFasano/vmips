
SHELL = @SHELL@

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

bindir = @bindir@
sbindir = @sbindir@
libexecdir = @libexecdir@
datadir = @datadir@
sysconfdir = @sysconfdir@
sharedstatedir = @sharedstatedir@
localstatedir = @localstatedir@
libdir = @libdir@
infodir = @infodir@
mandir = @mandir@
includedir = @includedir@
oldincludedir = /usr/include

DESTDIR =

pkgdatadir = $(datadir)/@PACKAGE@
pkglibdir = $(libdir)/@PACKAGE@
pkgincludedir = $(includedir)/@PACKAGE@

INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@ $(AM_INSTALL_PROGRAM_FLAGS)
INSTALL_DATA = @INSTALL_DATA@
INSTALL_SCRIPT = @INSTALL_SCRIPT@
transform = @program_transform_name@

DOC = README
TOC = toc
OPTDOC = options.texi
OPTIONTBLH = $(top_srcdir)/optiontbl.h
# This is less than optimal. We could consolidate CHAPTERS and DISTFILES
# more than they are currently.
CHAPTERS = $(srcdir)/delay \
 $(srcdir)/excprio \
 $(srcdir)/fix-devs \
 $(srcdir)/spim-console \
 $(srcdir)/cache \
 $(srcdir)/to-do \
 $(srcdir)/status-plans \
 $(srcdir)/euclid.txt \
 $(srcdir)/interrupt \
 $(srcdir)/linux-cross \
 $(srcdir)/egcs-bug \
 $(srcdir)/abistuff
CLEANFILES = vmips.log vmips.aux vmips.cp vmips.fn vmips.vr \
 vmips.tp vmips.ky vmips.pg vmips.toc
DISTCLEANFILES = 
USERDOCS = $(DOC) $(OPTDOC) $(TOC) vmips.dvi vmips.info vmips.html vmips.ps
MAINTAINERCLEANFILES = $(USERDOCS)
DISTFILES = Makefile.in cache delay egcs-bug euclid.txt excprio fix-devs \
 interrupt spim-console linux-cross abistuff \
 status-plans to-do vmips.texi $(USERDOCS)
mkinstalldirs = $(SHELL) $(top_srcdir)/mkinstalldirs


all: $(DOC) dvi info html ps

# Nothing to do here (yet):
check installcheck:

install: vmips.info
	$(mkinstalldirs) $(DESTDIR)$(infodir)
	$(INSTALL_DATA) $(srcdir)/vmips.info $(DESTDIR)$(infodir)/`echo vmips.info|sed '$(transform)'`

ps: vmips.ps

vmips.ps: vmips.dvi
	cd $(srcdir) && dvips -o vmips.ps vmips.dvi

dvi: vmips.dvi

vmips.dvi: $(srcdir)/vmips.texi $(OPTDOC)
	cd $(srcdir) && texi2dvi vmips.texi

info: vmips.info

vmips.info: $(srcdir)/vmips.texi $(OPTDOC)
	cd $(srcdir) && makeinfo vmips.texi

html: vmips.html

vmips.html: $(srcdir)/vmips.texi $(OPTDOC)
	cd $(srcdir) && makeinfo --html vmips.texi

$(DOC): $(TOC) $(CHAPTERS)
	cd $(srcdir) && cat $(TOC) $(CHAPTERS) > $(srcdir)/$(DOC)

$(TOC): $(CHAPTERS)
	cd $(srcdir) && ./maketoc -o $(TOC) $(CHAPTERS)

$(OPTDOC): $(OPTIONTBLH)
	cd $(srcdir) && ./makeoptdoc $(OPTDOC)

clean:
	-rm -f $(CLEANFILES)

distclean: clean
	-rm -f $(DISTCLEANFILES)

maintainer-clean: distclean clean
	-rm -f $(MAINTAINERCLEANFILES)

# Snarfed from automake and modified.
distdir: $(DISTFILES)
	-rm -rf $(distdir)
	mkdir $(distdir)
	-chmod 777 $(distdir)
	here=`cd $(top_builddir) && pwd`; \
	top_distdir=`cd $(distdir) && pwd`; \
	distdir=`cd $(distdir) && pwd`; \
	for file in $(DISTFILES); do \
	  d=$(srcdir); \
	  if test -d $$d/$$file; then \
	    cp -pr $$d/$$file $(distdir)/$$file; \
	  else \
	    test -f $(distdir)/$$file \
	    || ln $$d/$$file $(distdir)/$$file 2> /dev/null \
	    || cp -p $$d/$$file $(distdir)/$$file || :; \
	  fi; \
	done

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
